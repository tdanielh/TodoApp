{% for user in users %}
<div class="row py-2" data-userid="{{ user.id }}"  data-waiting="fa-spinner fa-pulse">
	<div class="col-11 justify-content-between">
		<div class="card">
			<div class="card-body">
				<span>{{ user.name }}</span>
			</div>
		</div>
	</div>
	<div class="col-1 align-self-center">
		<i class="fa fa-window-close fa-2x clickable unshare"></i>
	</div>
</div>

{% endfor %}